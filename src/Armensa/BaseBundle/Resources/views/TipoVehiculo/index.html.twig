{% extends '::base.html.twig' %}

{% block body -%}
    <div class="page-header">
    	<h1>TipoVehiculo</h1>     <a href="{{ path('tipovehiculo_new') }}" class="btn btn-primary btn-mini"> <i class="icon-plus icon-white"></i>
                Crear Nuevo
            </a>
        </div>
    <table class="data-tbl-simple table table-bordered table-condensed table-striped table-hover">
        <thead>
            <tr>

            	<th>Id</th>

            	<th>Tipo</th>

            	<th>Tarifa</th>
                <th>Acciones</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('tipovehiculo_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.tipo }}</td>
                <td>{{ entity.tarifa }}</td>
                <td style="text-align:center"><a href="{{ path('tipovehiculo_show', { 'id': entity.id }) }}" > <i class="icon-zoom-in"></i> </a><a href="{{ path('tipovehiculo_edit', { 'id': entity.id }) }}" ><i class="icon-edit"></i> </a>                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>


{% endblock %}
{% block javascripts -%}

{% endblock %}
