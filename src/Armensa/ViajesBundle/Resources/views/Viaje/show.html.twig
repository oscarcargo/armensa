{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Viaje</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Fecha</th>
                <td>{{ entity.fecha|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Fechacreacion</th>
                <td>{{ entity.fechaCreacion|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Origen</th>
                <td>{{ entity.origen }}</td>
            </tr>
            <tr>
                <th>Destino</th>
                <td>{{ entity.destino }}</td>
            </tr>
            <tr>
                <th>Valorcompra</th>
                <td>{{ entity.valorCompra }}</td>
            </tr>
            <tr>
                <th>Valorventa</th>
                <td>{{ entity.valorVenta }}</td>
            </tr>
            <tr>
                <th>Peso</th>
                <td>{{ entity.peso }}</td>
            </tr>
            <tr>
                <th>Cantidad</th>
                <td>{{ entity.cantidad }}</td>
            </tr>
            <tr>
                <th>Observciones</th>
                <td>{{ entity.observciones }}</td>
            </tr>
        </tbody>
    </table>

        <div class="form-actions">
        <a href="{{ path('viaje') }}" class="btn btn-mini">
            Cancelar        </a>
        <a href="{{ path('viaje_edit', { 'id': entity.id }) }}" class="btn btn-primary btn-mini">
            Editar
        </a>
{{ form(delete_form) }}
{% endblock %}
